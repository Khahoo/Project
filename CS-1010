import tkinter as tk
from tkinter import scrolledtext
mainwindow = tk.Tk()
mainwindow.title("Lab 03")
mainwindow.geometry("800x600")
mainwindow.columnconfigure(0, weight=1)
mainwindow.rowconfigure(1, weight=1)

def showRed():
    redFrame.tkraise()
def showGreen():
    greenFrame.tkraise()
def showBlue():
    blueFrame.tkraise()
def showYellow():
    yellowFrame.tkraise()
def showFifth():
    fifthFrame.tkraise()
def showSixth():
    sixthFrame.tkraise()
def showSeventh():
    seventhFrame.tkraise()
def showEighth():
    eighthFrame.tkraise()
def showNinth():
    ninthFrame.tkraise()
def showTenth():
    tenthFrame.tkraise()
def showEleven():
    eleventhFrame.tkraise()
def showThirteenth():
    ThirteenthFrame.tkraise()
def showFourthteen():
    FourthteenFrame.tkraise()

    
# Parent widget for the buttons
top_frame = tk.Frame(mainwindow, bg="cyan", highlightbackground="black", 
highlightthickness=1)
top_frame.grid(row=0, column=0, columnspan=20, sticky=tk.N+tk.W+tk.E)

reset = tk.Button(top_frame, text='RESET', width='5', command=showFourthteen)
reset.grid(row=0, column=0, padx=(10), pady=10)

